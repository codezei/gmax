section.works.block#works.custom-bg-light 
    .container
        h2.works__title.text-center.text-uppercase Prace
        h3.h6.text-center.text-light.works__subtitle Realne efekty, które mówią same za siebie.
        .works__categories
            .categories
                .categories__item
                    button.btn.btn-secondary.btn-small.active.category(type="button", data-slide="0") Pranie tapicerki meblowej
                .categories__item
                    button.btn.btn-secondary.btn-small.category(type="button", data-slide="1") Czyszczenie materacy
                .categories__item
                    button.btn.btn-secondary.btn-small.category(type="button", data-slide="2") Czyszczenie dywanów
                .categories__item
                    button.btn.btn-secondary.btn-small.category(type="button", data-slide="3") Pranie tapicerki samochodowej
                .categories__item
                    button.btn.btn-secondary.btn-small.category(type="button", data-slide="4") Mycie i polerowanie szyb
                .categories__item
                    button.btn.btn-secondary.btn-small.category(type="button", data-slide="5") Czyszczenie zasłon i rolet
        .swiper.works-swiper
            .swiper-wrapper 
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/furniture/furniture-img-${num}.webp`) 
                                picture.work
                                    source(srcset=`img/furniture/furniture-img-${num}-mob.webp`, type="image/webp")
                                    img.work__img(loading="lazy", src=`img/furniture/furniture-img-${num}-mob.jpg`, alt="")
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/mattress/mattress-img-${num}.webp`) 
                                picture.work
                                    source(srcset=`img/mattress/mattress-img-${num}-mob.webp`, type="image/webp")
                                    img.work__img(loading="lazy", src=`img/mattress/mattress-img-${num}-mob.jpg`, alt="")
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/carpet/carpet-img-${num}.webp`) 
                                picture.work
                                    source(srcset=`img/carpet/carpet-img-${num}-mob.webp`, type="image/webp")
                                    img.work__img(loading="lazy", src=`img/carpet/carpet-img-${num}-mob.jpg`, alt="")
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/salon/salon-img-${num}.webp`) 
                                picture.work
                                    source(srcset=`img/salon/salon-img-${num}-mob.webp`, type="image/webp")
                                    img.work__img(loading="lazy", src=`img/salon/salon-img-${num}-mob.jpg`, alt="")
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/window/window-img-${num}.webp`) 
                                picture.work
                                    source(srcset=`img/window/window-img-${num}-mob.webp`, type="image/webp")
                                    img.work__img(loading="lazy", src=`img/window/window-img-${num}-mob.jpg`, alt="")
                .swiper-slide
                    .row.gallery.row-cols-2.row-cols-sm-2.row-cols-md-4
                        each num in [1,2,3,4]
                            a.col(href=`img/curtain/curtain-img-${num}.webp`) 
